{
  "env": {
    "AIRTABLE_API_KEY": "@airtable-api-key",
    "AIRTABLE_BASE_ID": "tblydYnNRTUmEr6vj"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/grikomsn/shrtn"
    }
  ],
  "rewrites": [
    {
      "source": "/:uid",
      "destination": "/api/redirect"
    }
  ],
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true
}
